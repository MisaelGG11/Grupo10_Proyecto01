<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UsuarioPerfilActivity"
    tools:layout_editor_absoluteX="-2dp"
    tools:layout_editor_absoluteY="-4dp">

  <ImageView
      android:id="@+id/imageView2"
      android:layout_width="61dp"
      android:layout_height="50dp"
      android:layout_marginTop="24dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/user"
      tools:ignore="ContentDescription" />

  <TextView
      android:id="@+id/tvNombre"
      android:layout_width="332dp"
      android:layout_height="24dp"
      android:layout_marginTop="20dp"
      android:text="@string/nombre_perfil"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.493"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView2"
      tools:ignore="TextSizeCheck" />

  <TextView
      android:id="@+id/tvEmail"
      android:layout_width="332dp"
      android:layout_height="24dp"
      android:layout_marginTop="16dp"
      android:text="@string/email_perfil"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.493"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/edtNombre"
      tools:ignore="TextSizeCheck" />

  <TextView
      android:id="@+id/tvTelefono"
      android:layout_width="332dp"
      android:layout_height="24dp"
      android:layout_marginTop="16dp"
      android:text="@string/telefono_perfil"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.493"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/edtEmail"
      tools:ignore="TextSizeCheck,TextSizeCheck" />

  <TextView
      android:id="@+id/tvCategoria"
      android:layout_width="332dp"
      android:layout_height="24dp"
      android:layout_marginTop="16dp"
      android:text="@string/categoria_perfil"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.493"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/edtTelefono"
      tools:ignore="TextSizeCheck" />

  <EditText
      android:id="@+id/edtNombre"
      android:layout_width="332dp"
      android:layout_height="42dp"
      android:layout_marginTop="4dp"
      android:ems="10"
      android:enabled="false"
      android:importantForAutofill="no"
      android:inputType="textPersonName"
      android:textSize="14sp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.493"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/tvNombre"
      tools:ignore="LabelFor,TouchTargetSizeCheck,SpeakableTextPresentCheck" />

  <EditText
      android:id="@+id/edtEmail"
      android:layout_width="332dp"
      android:layout_height="48dp"
      android:layout_marginTop="4dp"
      android:ems="10"
      android:enabled="false"
      android:inputType="textPersonName"
      android:textSize="14sp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.493"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/tvEmail"
      tools:ignore="LabelFor,SpeakableTextPresentCheck,TextFields"
      android:importantForAutofill="no" />

  <EditText
      android:id="@+id/edtTelefono"
      android:layout_width="332dp"
      android:layout_height="48dp"
      android:layout_marginTop="4dp"
      android:ems="10"
      android:enabled="false"
      android:inputType="textPersonName"
      android:textSize="14sp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.493"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/tvTelefono"
      tools:ignore="LabelFor,SpeakableTextPresentCheck"
      android:importantForAutofill="no" />

  <EditText
      android:id="@+id/edtTCategoria"
      android:layout_width="332dp"
      android:layout_height="48dp"
      android:layout_marginTop="4dp"
      android:ems="10"
      android:enabled="false"
      android:inputType="textPersonName"
      android:textSize="14sp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.493"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/tvCategoria"
      tools:ignore="LabelFor,SpeakableTextPresentCheck"
      android:importantForAutofill="no" />

</androidx.constraintlayout.widget.ConstraintLayout>